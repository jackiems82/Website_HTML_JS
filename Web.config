<?xml version="1.0"?>

<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".otf" mimeType="font/otf" />
     </staticContent>
    </system.webServer>
    <system.web>
      <customErrors mode="Off"/>
   </system.web>
    <rewrite>
        <rules>
            <rule name="Redirect galleries">
                <match url="galleries/([0-9]+)/([0-9]+)" />
                <!-- <conditions>
                    <add input="{HTTP_HOST}" pattern="localhost" negate="true" />
                </conditions> -->
                <action type="Redirect" url="projects.html#{R:1}&amp;gid={R:1}&amp;pid={R:2}" />
            </rule>
        </rules>
    </rewrite>
</configuration> 